{
  "expo": {
    "name": "react-natve-expo-2024-app",
    "slug": "react-natve-expo-2024-app",
    "scheme": "react-natve-expo-2024-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/imagem.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5dc"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/imagem.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ]
  }
}
